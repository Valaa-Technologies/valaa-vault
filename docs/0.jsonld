    "section_types": {
      "@id": "section_types",
      "@type": "VDoc:Chapter",
      "dc:title": [
        {
          "VDoc:em": true,
          "VDoc:content": [
            "V"
          ]
        },
        " ",
        {
          "@type": "VDoc:Reference",
          "VDoc:content": [
            "valospace resource types"
          ],
          "VDoc:ref": "VState:Type"
        }
      ],
