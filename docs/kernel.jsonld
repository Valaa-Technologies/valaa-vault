        "fabricAbstract": {
          "VDoc:content": [
            {
              "@type": "VDoc:Paragraph",
              "VDoc:content": [
                "'VEngine' namespace provides vocabulary for describing and defining\nthe valosheath and its types, objects, properties, methods and constants."
              ]
            }
          ]
        },
        "VRevdoc:preferredPrefix": "VEngine",
        "VRevdoc:baseIRI": "https://valospace.org/engine/0#",
        "VRevdoc:referencedModules": {
          "https://valospace.org/engine/0#": "/home/iridian/valaa/kernel/dist/packages/@valos/engine/VEngine",
          "https://valospace.org/kernel/0#": "/home/iridian/valaa/kernel/dist/packages/@valos/kernel/VKernel",
          "https://valospace.org/raem/0#": "/home/iridian/valaa/kernel/dist/packages/@valos/raem/VModel"
        },
        "VRevdoc:extenderModules": {
