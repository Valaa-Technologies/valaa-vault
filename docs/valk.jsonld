        {
          "@type": "VDoc:Paragraph",
          "VDoc:content": [
            {
              "VDoc:em": true,
              "VDoc:content": [
                "VValk"
              ]
            },
            " together with ",
            {
              "@type": "VDoc:Reference",
              "VDoc:content": [
                "VState:"
              ],
              "VDoc:ref": "VState:"
            },
            " ",
            {
              "@type": "VDoc:Reference",
              "VDoc:content": [
                "VLog:"
              ],
              "VDoc:ref": "VLog:"
            },
            " and ",
            {
              "@type": "VDoc:Reference",
              "VDoc:content": [
                "VPlot:"
              ],
              "VDoc:ref": "VPlot:"
            },
            " form the infrastructural foundation ie. the ",
            {
              "VDoc:em": true,
              "VDoc:content": [
                "fabric"
              ]
            },
            " of the valospace.",
            {
              "VDoc:em": true,
              "VDoc:content": [
                "VValk"
              ]
            },
            " provides the intermediate language used for declarative\nand imperative logic alike."
          ]
        }
      ]
    },
