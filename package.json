{
  "name": "@valos/kernel",
  "version": "0.35.0-prerelease.13",
  "valos": {
    "type": "vault",
    "domain": "@valos/kernel",
    "workspacePrefix": "@valos/"
  },
  "description": "ValOS common infrastructure tools and libraries monorepository.",
  "license": "MIT",
  "author": "Valaa Technologies Ltd",
  "private": true,
  "repository": "https://github.com/valaatech/kernel",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "vlm": "vlm ",
    "start": "vlm rouse-revealer ",
    "dev": "TARGET_ENV=local vlm rouse-revealer ",
    "prod": "TARGET_ENV=production vlm rouse-revealer ",
    "clean": "lerna clean --yes",
    "eslint": "eslint .",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "npm run eslint",
    "test": "jest",
    "build": "webpack ",
    "assemble-packages": "vlm assemble-packages ",
    "publish-packages": "vlm publish-packages "
  },
  "devDependencies": {
    "@cyclonedx/bom": "^0.2.6",
    "@valos/inspire": "^0.35.0-prerelease.10",
    "@valos/kernel": "^0.35.0-prerelease.10",
    "@valos/toolset-revealer": "^0.35.0-prerelease.10",
    "@valos/type-vault": "^0.35.0-prerelease.10",
    "valma": "^0.35.0-prerelease.10",
    "xml-js": "^1.6.11"
  },
  "dependencies": {
    "@valos/engine": "file:.yalc/@valos/engine",
    "@valos/gateway-api": "file:.yalc/@valos/gateway-api",
    "@valos/hypertwin": "file:.yalc/@valos/hypertwin",
    "@valos/inspire": "file:.yalc/@valos/inspire",
    "@valos/kernel": "file:.yalc/@valos/kernel",
    "@valos/query-authority": "file:.yalc/@valos/query-authority",
    "@valos/raem": "file:.yalc/@valos/raem",
    "@valos/revdoc": "file:.yalc/@valos/revdoc",
    "@valos/sbomdoc": "file:.yalc/@valos/sbomdoc",
    "@valos/script": "file:.yalc/@valos/script",
    "@valos/sourcerer": "file:.yalc/@valos/sourcerer",
    "@valos/tools": "file:.yalc/@valos/tools",
    "@valos/toolset-rest-api-gateway-plugin": "file:.yalc/@valos/toolset-rest-api-gateway-plugin",
    "@valos/twindoc": "file:.yalc/@valos/twindoc",
    "@valos/type-gateway-plugin": "file:.yalc/@valos/type-gateway-plugin",
    "@valos/type-opspace": "file:.yalc/@valos/type-opspace",
    "@valos/type-worker": "file:.yalc/@valos/type-worker",
    "@valos/vdoc": "file:.yalc/@valos/vdoc"
  }
